---
import RecentWork from "./RecentWork.astro";
const {projects} = Astro.props;
---

<section class='work'>
	<div class='work-container'>
		<h2 class='work-title'>Recent Work</h2>
		<ul class='recent-work-list'>
			{projects.map((work: any) => <RecentWork work={work} />)}
		</ul>
	</div>
</section>
<style lang='scss'>
	@use "../styles/components/RecentWorkSection.scss";
</style>
